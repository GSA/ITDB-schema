  <xsd:simpleType name="CostPoolCategorySimpleType">
    <xsd:annotation>
      <xsd:documentation>Cost Pool categories</xsd:documentation>
      <xsd:appinfo>
        <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Total Internal Labor Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY cost represented by the number of government FTEs associated with the Investment.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total External Labor Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY cost for external labor; contractor fees, travel and expenses, associated with this Investment.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Outside Services Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY cost for outside services including consulting, managed service providers and cloud service providers.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Hardware Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY cost for hardware including expense, lease, maintenance and support, and depreciation and amortization.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Software Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY cost for software including expense, subscription, maintenance and support, and depreciation and amortization.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Facilities and Power Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BYcost for facilities and power including expense, lease, maintenance and support, and depreciation and amortization.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Telecom Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BYcost for telecom including expense, lease, maintenance and support, and depreciation and amortization.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Other Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total other cost for PY/CY/BY including miscellaneous or non-standard expenses.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Total Internal Services Amount">
        <xsd:annotation>
          <xsd:documentation>This is the total PY/CY/BY internal services cost for shared services charges received from other internal shared services groups.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
