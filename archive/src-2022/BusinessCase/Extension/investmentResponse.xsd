    </sequence>
  </complexType>

  <element name="investmentsEliminatedorReduced">
    <complexType>
      <sequence>
        <element ref="businessCase-response:investmentEliminatedorReduced" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="investmentEliminatedorReduced">
    <complexType>
      <sequence>
