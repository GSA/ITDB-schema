      <element name="existingMetric" type="tns:addExistingMetricType" minOccurs="0" />
      <element name="deploymentTarget" type="businessCase-type:longTextString"/>
    </sequence>
  </complexType>

  <complexType name="addNewMetricType">
    <sequence>
      <element name="description" type="businessCase-type:longTextString"/>
      <element name="unitOfMeasure" type="businessCase-type:shortTextString"/>
      <element name="measurementCondition" type="businessCase-facet:prmConditionTypes"/>
    </sequence>
  </complexType>

  <complexType name="addExistingMetricType">
    <sequence>
      <element name="metricID" type="integer" minOccurs="0"/>
      <element name="agencyMetricID" type="businessCase-type:shortTextString" minOccurs="0"/>
    </sequence>
  </complexType>
  -->
  <complexType name="addProjectActivitiesType">
    <sequence>
      <element name="addProjectActivity" type="tns:addProjectActivityType" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="addProjectActivityType">
    <sequence>
      <element name="agencyActivityId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="activityName" type="businessCase-type:string100"/>
      <element name="activityDescription" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="structureId" type="string"/>
      <element name="plannedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="projectedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="actualTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="plannedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addAgileProjectActivitiesType">
    <sequence>
      <element name="addAgileProjectActivity" type="tns:addAgileProjectActivityType" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="addAgileProjectActivityType">
    <sequence>
      <element name="agencyActivityId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="releaseName" type="businessCase-type:shortTextString"/>
      <element name="releaseNumber" type="businessCase-type:string50"/>
      <element name="releaseDescription" type="businessCase-type:longTextString"/>
      <element name="NPI" type="businessCase-type:agileSprints" minOccurs="0"/>
      <element name="NPE" type="businessCase-type:agileEpics" minOccurs="0"/>
      <element name="NCE" type="businessCase-type:agileEpics" minOccurs="0"/>
      <element name="NCI" type="businessCase-type:agileSprints" minOccurs="0"/>
      <element name="DTC" type="businessCase-type:agileContributors" minOccurs="0"/>
      <element name="DPC" type="businessCase-type:agileContributors" minOccurs="0"/>
      <element name="plannedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="projectedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="actualTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="updatePerformanceMetricType">
    <sequence>
      <element name="performanceMetricId" type="integer" minOccurs="0"/>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0" nillable="true"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="addInvestmentRiskType">
    <sequence>
      <element name="agencyInvestmentRiskId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="investmentRiskName" type="businessCase-type:mediumTextString"/>
      <element name="investmentRiskArea" type="businessCase-facet:riskType"/>
      <element name="investmentRiskProbability" type="businessCase-facet:impactType"/>
      <element name="investmentRiskImpact" type="businessCase-facet:impactType"/>
      <element name="investmentRiskMitigationPlan" type="businessCase-type:string1000"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="updateInvestmentRiskType">
    <sequence>
      <element name="investmentRiskId" type="integer" minOccurs="0"/>
      <element name="agencyInvestmentRiskId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="investmentRiskName" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="investmentRiskArea" type="businessCase-facet:riskType" minOccurs="0"/>
      <element name="investmentRiskProbability" type="businessCase-facet:impactType" minOccurs="0"/>
      <element name="investmentRiskImpact" type="businessCase-facet:impactType" minOccurs="0"/>
      <element name="investmentRiskMitigationPlan" type="businessCase-type:string1000" minOccurs="0"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="updateCIORatingType">
    <sequence>
      <element name="cioRatingEval" type="businessCase-type:cioRatingType"/>
      <element name="cioComment" type="businessCase-type:longTextString"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="reviseInvestmentType">
    <sequence>
      <element name="baselineApprovalDate" type="businessCase-type:investmentDateType"/>
      <element name="reviseOption" type="businessCase-facet:reviseOptionType"/>
      <element name="reviseComments" type="string"/>
      <element name="reviseAddProject" type="tns:reviseAddProjectType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdateProject" type="tns:reviseUpdateProjectType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseAddPerformanceMetric" type="tns:reviseAddPerformanceMetricType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdatePerformanceMetric" type="tns:reviseUpdatePerformanceMetricType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdateFundingDetails" type="tns:reviseUpdateFundingDetailsType" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="reviseUpdateFundingDetailsType">
    <sequence>
      <element name="lifeCycleCosts" type="tns:lifeCycleCostsType" minOccurs="0"/>
      <element name="investmentStartEndDates" type="tns:investmentStartEndDatesType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="updateFundingDetailsType">
    <sequence>
      <element name="lifeCycleCosts" type="tns:lifeCycleCostsType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddPerformanceMetricType">
    <sequence>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricDescription" type="businessCase-type:mediumTextString"/>
      <element name="performanceMetricUOM" type="businessCase-type:string50"/>
      <element name="performanceMetricCategory" type="businessCase-facet:metricCategoryTypes"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricPYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType"/>
      <element name="performanceMetricCondition" type="businessCase-facet:prmConditionTypes"/>
