      <element name="performanceMetricCategory" type="businessCase-facet:metricCategoryTypes" minOccurs="0"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricPYTarget" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="performanceMetricCondition" type="businessCase-facet:prmConditionTypes" minOccurs="0"/>
      <element name="performancePeriodicity" type="businessCase-facet:measureReportingFrequencyType" minOccurs="0"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdatePerformanceMetricActual" type="tns:reviseUpdatePerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0" nillable="true"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addPerformanceMetricActualType">
    <sequence>
      <element name="agencyPerformanceMetricActualID" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricActualResult" type="businessCase-type:metricUnitType"/>
      <element name="performanceMetricDateofActual" type="businessCase-type:investmentDateType"/>
      <element name="performanceMetricComment" type="businessCase-type:mediumTextString" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseUpdatePerformanceMetricActualType">
    <sequence>
      <element name="performanceMetricActualID" type="integer" minOccurs="0"/>
      <element name="agencyPerformanceMetricActualID" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricActualResult" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="performanceMetricDateofActual" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="performanceMetricComment" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectType">
    <sequence>
      <element name="agencyProjectId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="reviseAddProjectDetail" type="tns:reviseAddProjectDetailType"/>
      <!--<element name="reviseAddProjectMetric" type="tns:reviseAddProjectMetricType"/>-->
      <element name="reviseAddProjectActivities" type="tns:reviseAddProjectActivitiesType"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectDetailType">
    <sequence>
      <element name="uniqueProjectId" type="businessCase-type:string10"/>
      <element name="projectName" type="businessCase-type:string100"/>
      <element name="projectGoal" type="businessCase-type:shortTextString"/>
      <element name="projectStartDate" type="businessCase-type:investmentDateType"/>
      <element name="projectCompletionDate" type="businessCase-type:investmentDateType"/>
      <element name="projectLifeCycleCost" type="businessCase-type:dollarAmount"/>
      <element name="softwareProject" type="businessCase-facet:yesNoString"/>
    </sequence>
  </complexType>

