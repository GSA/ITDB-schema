<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.itdashboard.gov/service"
        xmlns:tns="http://www.itdashboard.gov/service"
        xmlns:businessCase-add="urn:us:gov:businessCase-add"
        xmlns:businessCase-update="urn:us:gov:businessCase-update"
        xmlns:businessCase-type="urn:us:gov:businessCase-type"
        xmlns:businessCase-facet="urn:us:gov:businessCase-facet"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        elementFormDefault="qualified"
        version="FY_2019">

  <annotation>
    <documentation>
      1. This schema should be used to Submit Data to ITDashboard.
      2. Any dates that are referred in this schema should be in 'YYYY-MM-DD' format.
    </documentation>
  </annotation>

  <import namespace="urn:us:gov:businessCase-add" schemaLocation="../Extension/addInvestment.xsd"/>
  <import namespace="urn:us:gov:businessCase-update" schemaLocation="../Extension/updateInvestment.xsd"/>
  <import namespace="urn:us:gov:businessCase-type" schemaLocation="../Subset/businessCaseTypes.xsd"/>
  <import namespace="urn:us:gov:businessCase-facet" schemaLocation="../Subset/businessCaseFacet.xsd"/>

  <element name="operations" type="tns:operationsType"/>

  <complexType name="operationsType">
    <sequence>
      <element name="investmentInformation" type="tns:investmentInformationType" maxOccurs="unbounded"/>
    </sequence>
    <attribute name="validateOnlyIndicator" type="boolean"/>
  </complexType>

  <complexType name="investmentInformationType">
    <choice>
      <element name="getInformation" type="tns:getInvestmentInformationType"/>
      <element name="monthlySubmission" type="tns:monthlySubmissionType"/>
      <element name="annualSubmission" type="tns:annualSubmissionType"/>
    </choice>
    <attribute name="uniqueInvestmentIdentifier" type="businessCase-type:investmentIdentifierType" use="required"/>
  </complexType>

  <complexType name="getInvestmentInformationType">
    <sequence>
      <element name="getInvestment" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            Use this operation to get investment details.
          </documentation>
        </annotation>
      </element>
      <element name="getCIORating" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used get CIO rating.
          </documentation>
        </annotation>
      </element>
      <element name="getContracts" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used get Contracts.
          </documentation>
        </annotation>
      </element>
      <element name="getAcquisitions" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used get Contracts.
          </documentation>
        </annotation>
      </element>
      <element name="getSystemsInventory" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used get Systems Inventory.
          </documentation>
        </annotation>
      </element>
      <element name="getPerformanceMetrics" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to get Performance Metrics.
          </documentation>
        </annotation>
      </element>
      <element name="getInvestmentRisks" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to get Operation Risks.
          </documentation>
        </annotation>
      </element>
      <element name="getProjects" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to get Projects information.
          </documentation>
        </annotation>
      </element>
      <element name="getProjectsInProgress" type="tns:getInformationType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to get In-Progress Projects information.
          </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>

  <complexType name="annualSubmissionType">
    <sequence>
      <element name="updateInvestment" type="tns:updateInvestmentType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to update an investment.
            Should be used for annual updates only.
          </documentation>
        </annotation>
      </element>
      <element name="addInvestment" type="tns:addInvestmentType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used add a new investment.
            Should be used for annual updates only.
          </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>

  <complexType name="monthlySubmissionType">
    <sequence>
      <element name="updateContract" type="tns:updateContractType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used update Contracts.
          </documentation>
        </annotation>
      </element>
      <element name="addContract" type="tns:addContractType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to add Contracts.
          </documentation>
        </annotation>
      </element>
      <element name="updateAcquisition" type="tns:updateAcquisitionType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used update Acquisitions.
          </documentation>
        </annotation>
      </element>
      <element name="addAcquisition" type="tns:addAcquisitionType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to add Acquisitions.
          </documentation>
        </annotation>
      </element>
      <element name="updateSystem" type="tns:updateSystemType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used update Systems Inventory.
          </documentation>
        </annotation>
      </element>
      <element name="addSystem" type="tns:addSystemType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to add Systems Inventory.
          </documentation>
        </annotation>
      </element>
      <element name="addPerformanceMetric" type="tns:addPerformanceMetricType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used add Performance Metrics.
          </documentation>
        </annotation>
      </element>
      <element name="updatePerformanceMetric" type="tns:updatePerformanceMetricType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used update Performance Metrics.
          </documentation>
        </annotation>
      </element>
      <element name="updateCIORating" type="tns:updateCIORatingType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used update CIO rating.
          </documentation>
        </annotation>
      </element>
      <element name="updateOperationalAnalysis" type="tns:updateOperationalAnalysisType" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to update Operational Analysis.
          </documentation>
        </annotation>
      </element>
      <element name="updateInvestmentRisk" type="tns:updateInvestmentRiskType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to update Investment Risk.
          </documentation>
        </annotation>
      </element>
      <element name="addInvestmentRisk" type="tns:addInvestmentRiskType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to add Investment Risk.
          </documentation>
        </annotation>
      </element>
      <element name="addProject" type="tns:addProjectType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to add project information.
          </documentation>
        </annotation>
      </element>
      <element name="updateProject" type="tns:updateProjectType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to update project information.
          </documentation>
        </annotation>
      </element>
      <element name="reviseInvestment" type="tns:reviseInvestmentType" maxOccurs="unbounded" minOccurs="0">
        <annotation>
          <documentation>
            This operation is used to revise an investment.
          </documentation>
        </annotation>
      </element>
      <element name="updateInvestmentDetails" type="tns:updateInvestmentDetailsType" minOccurs="0">
        <!-- Doesn't have an addInvestmentDetails operation because it is handled in the annualSubmissionType, under capitalAssetPlan > capitalAssetSummary > InvestmentDetails. -->
        <annotation>
          <documentation>
            Use this operation to update a subset of investment data.
          </documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>

  <complexType name="operationsOut">
    <choice>
      <element name="getInvestment" type="tns:getInformationType"/>
      <element name="getCIORating" type="tns:getInformationType"/>
      <element name="getContracts" type="tns:getInformationType"/>
      <element name="getAcquisitions" type="tns:getInformationType"/>
      <element name="getSystemsInventory" type="tns:getInformationType"/>
      <element name="getInvestmentRisks" type="tns:getInformationType"/>
      <element name="getProjects" type="tns:getInformationType"/>
      <element name="getProjectsInProgress" type="tns:getInformationType"/>
      <element name="updateContract" type="tns:updateContractType"/>
      <element name="addContract" type="tns:addContractType"/>
      <element name="updateAcquisition" type="tns:updateAcquisitionType"/>
      <element name="addAcquisition" type="tns:addAcquisitionType"/>
      <element name="updateSystem" type="tns:updateSystemType"/>
      <element name="addSystem" type="tns:addSystemType"/>
      <element name="updateReplacedContract" type="tns:updateReplacedContractType"/>
      <element name="addReplacedContract" type="tns:addReplacedContractType"/>
      <element name="addPerformanceMetric" type="tns:addPerformanceMetricType"/>
      <element name="updatePerformanceMetric" type="tns:updatePerformanceMetricType"/>
      <element name="updateCIORating" type="tns:updateCIORatingType"/>
      <element name="updateOperationalAnalysis" type="tns:updateOperationalAnalysisType"/>
      <element name="updateInvestmentRisk" type="tns:updateInvestmentRiskType"/>
      <element name="addInvestmentRisk" type="tns:addInvestmentRiskType"/>
      <element name="addProject" type="tns:addProjectType"/>
      <element name="updateProject" type="tns:updateProjectType"/>
      <element name="reviseInvestment" type="tns:reviseInvestmentType"/>
      <element name="updateInvestment" type="tns:updateInvestmentType"/>
      <element name="addInvestment" type="tns:addInvestmentType"/>
      <element name="updateInvestmentDetails" type="tns:updateInvestmentDetailsType"/>
    </choice>
  </complexType>

  <complexType name="addPerformanceMetricType">
    <sequence>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricDescription" type="businessCase-type:mediumTextString"/>
      <element name="performanceMetricUOM" type="businessCase-type:string50"/>
      <element name="performanceMetricCategory" type="businessCase-facet:metricCategoryTypes"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricPYTarget" type="businessCase-type:metricUnitType" minOccurs='0'/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType"/>
      <element name="performanceMetricCondition" type="businessCase-facet:prmConditionTypes"/>
      <element name="performancePeriodicity" type="businessCase-facet:measureReportingFrequencyType"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string"/>
  </complexType>

  <complexType name="addProjectType">
    <sequence>
      <element name="agencyProjectId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="addProjectDetail" type="tns:addProjectDetailType"/>
      <element name="addProjectActivities" type="tns:addProjectActivitiesType"/>
    </sequence>
    <attribute name="id" type="string"/>
  </complexType>

  <complexType name="addProjectDetailType">
    <sequence>
      <element name="uniqueProjectId" type="businessCase-type:string10"/>
      <element name="projectName" type="businessCase-type:shortTextString"/>
      <element name="objectivesExpectedOutcomes" type="businessCase-type:longTextString"/>
      <element name="projectStartDate" type="businessCase-type:investmentDateType"/>
      <element name="projectCompletionDate" type="businessCase-type:investmentDateType"/>
      <element name="projectLifeCycleCost" type="businessCase-type:dollarAmount"/>
      <element name="SDLCmethodology" type="businessCase-facet:SDLCmethodologyType" minOccurs="0"/>
      <element name="releaseEverySixMo" type="businessCase-facet:yesNoNAType" minOccurs="0"/>
      <element name="releaseEverySixMoComment" type="businessCase-type:string1000" minOccurs="0"/>
      <element name="productionDeploymentDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addProjectActivitiesType">
    <sequence>
      <element name="addProjectActivity" type="tns:addProjectActivityType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="addAgileProjectActivity" type="tns:addAgileProjectActivityType" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="addProjectActivityType">
    <sequence>
      <element name="agencyActivityId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="activityName" type="businessCase-type:shortTextString"/>
      <element name="activityDescription" type="businessCase-type:longTextString" minOccurs="0"/>
      <element name="structureId" type="string"/>
      <element name="activityType" type="businessCase-facet:activityType" minOccurs="0"/>
      <element name="criticalPath" type="businessCase-facet:yesNoString" minOccurs="0"/>
      <element name="plannedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="projectedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="actualTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="plannedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addAgileProjectActivitiesType">
    <sequence>
      <element name="addAgileProjectActivity" type="tns:addAgileProjectActivityType" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="addAgileProjectActivityType">
    <sequence>
      <element name="agencyActivityId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="releaseName" type="businessCase-type:shortTextString"/>
      <element name="releaseNumber" type="businessCase-type:string50"/>
      <element name="releaseDescription" type="businessCase-type:longTextString"/>
      <element name="NPI" type="businessCase-type:agileSprints" minOccurs="0"/>
      <element name="NPE" type="businessCase-type:agileEpics" minOccurs="0"/>
      <element name="NCE" type="businessCase-type:agileEpics" minOccurs="0"/>
      <element name="NCI" type="businessCase-type:agileSprints" minOccurs="0"/>
      <element name="DTC" type="businessCase-type:agileContributors" minOccurs="0"/>
      <element name="DPC" type="businessCase-type:agileContributors" minOccurs="0"/>
      <element name="plannedStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualStartDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="projectedCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="actualCompletionDate" type="businessCase-type:investmentDateType" minOccurs="0"/>
      <element name="plannedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="projectedTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
      <element name="actualTotalCost" type="businessCase-type:dollarAmount" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="updatePerformanceMetricType">
    <sequence>
      <element name="performanceMetricId" type="integer" minOccurs="0"/>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0" nillable="true"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="addInvestmentRiskType">
    <sequence>
      <element name="agencyInvestmentRiskId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="investmentRiskName" type="businessCase-type:mediumTextString"/>
      <element name="investmentRiskCategory" type="businessCase-facet:riskType"/>
      <element name="investmentRiskProbability" type="businessCase-facet:impactType"/>
      <element name="investmentRiskImpact" type="businessCase-facet:impactType"/>
      <element name="investmentRiskMitigationPlan" type="businessCase-type:mediumTextString"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="updateInvestmentRiskType">
    <sequence>
      <element name="investmentRiskId" type="integer" minOccurs="0"/>
      <element name="agencyInvestmentRiskId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="investmentRiskName" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="investmentRiskCategory" type="businessCase-facet:riskType" minOccurs="0"/>
      <element name="investmentRiskProbability" type="businessCase-facet:impactType" minOccurs="0"/>
      <element name="investmentRiskImpact" type="businessCase-facet:impactType" minOccurs="0"/>
      <element name="investmentRiskMitigationPlan" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="updateCIORatingType">
    <sequence>
      <element name="cioRatingEval" type="businessCase-type:cioRatingType"/>
      <element name="cioComment" type="businessCase-type:longTextString"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="updateContractType">
    <sequence>
      <element name="contractId" type="integer" minOccurs="0"/>
      <element name="agencyContractId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="contractNumber" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="referencedPIID" type="businessCase-type:referencedPIIDType" minOccurs="0"/>
      <element name="isModularApproach" type="businessCase-facet:yesNoString" minOccurs="0"/>
      <element name="agileDevelopment" type="businessCase-facet:yesNoString" minOccurs="0"/>
      <element name="contractPurpose" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="ITLeaseCategory" type="businessCase-facet:ITLeaseCategoryType" minOccurs="0" nillable="true"/>
      <element name="informationSecurityClause" type="businessCase-facet:yesNoString" minOccurs="0"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="reviseInvestmentType">
    <sequence>
      <element name="baselineApprovalDate" type="businessCase-type:investmentDateType"/>
      <element name="reviseOption" type="businessCase-facet:reviseOptionType"/>
      <element name="reviseComments" type="string"/>
      <element name="reviseAddProject" type="tns:reviseAddProjectType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdateProject" type="tns:reviseUpdateProjectType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseAddPerformanceMetric" type="tns:reviseAddPerformanceMetricType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdatePerformanceMetric" type="tns:reviseUpdatePerformanceMetricType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdateFundingDetails" type="tns:reviseUpdateFundingDetailsType" minOccurs="0"/>
    </sequence>
    <attribute name="id" type="string" use="required"/>
  </complexType>

  <complexType name="reviseUpdateFundingDetailsType">
    <sequence>
      <element name="lifeCycleCosts" type="tns:lifeCycleCostsType" minOccurs="0"/>
      <element name="investmentStartEndDates" type="tns:investmentStartEndDatesType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="updateFundingDetailsType">
    <sequence>
      <element name="lifeCycleCosts" type="tns:lifeCycleCostsType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddPerformanceMetricType">
    <sequence>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricDescription" type="businessCase-type:mediumTextString"/>
      <element name="performanceMetricUOM" type="businessCase-type:string50"/>
      <element name="performanceMetricCategory" type="businessCase-facet:metricCategoryTypes"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricPYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType"/>
      <element name="performanceMetricCondition" type="businessCase-facet:prmConditionTypes"/>
      <element name="performancePeriodicity" type="businessCase-facet:measureReportingFrequencyType"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseUpdatePerformanceMetricType">
    <sequence>
      <element name="performanceMetricId" type="integer" minOccurs="0"/>
      <element name="agencyPerformanceMetricId" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricDescription" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="performanceMetricUOM" type="businessCase-type:string50" minOccurs="0"/>
      <element name="performanceMetricCategory" type="businessCase-facet:metricCategoryTypes" minOccurs="0"/>
      <element name="performanceMetricBaselineCapability" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricPYTarget" type="businessCase-type:metricUnitType" minOccurs="0" nillable="true"/>
      <element name="performanceMetricCYTarget" type="businessCase-type:metricUnitType" minOccurs="0"/>
      <element name="performanceMetricCondition" type="businessCase-facet:prmConditionTypes" minOccurs="0"/>
      <element name="performancePeriodicity" type="businessCase-facet:measureReportingFrequencyType" minOccurs="0"/>
      <element name="addPerformanceMetricActual" type="tns:addPerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="reviseUpdatePerformanceMetricActual" type="tns:reviseUpdatePerformanceMetricActualType" minOccurs="0" maxOccurs="unbounded"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0" nillable="true"/>
      <element name="isActive" type="businessCase-facet:isActiveString" minOccurs="0"/>
      <element name="isClosed" type="businessCase-facet:yesNoString" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addPerformanceMetricActualType">
    <sequence>
      <element name="agencyPerformanceMetricActualID" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricActualResult" type="businessCase-type:metricUnitType"/>
      <element name="performanceMetricDateofActual" type="businessCase-type:investmentDateType"/>
      <element name="performanceMetricComment" type="businessCase-type:mediumTextString" minOccurs="0"/>
      <element name="performanceMetricObjectiveOrGoal" type="businessCase-type:objectiveOrGoalType" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="reviseUpdatePerformanceMetricActualType">
    <sequence>
      <element name="performanceMetricActualID" type="integer" minOccurs="0"/>
      <element name="agencyPerformanceMetricActualID" type="businessCase-type:shortTextString" minOccurs="0"/>
      <element name="performanceMetricActualResult" type="businessCase-type:metricUnitType" minOccurs="0"/>
