<schema targetNamespace="urn:us:gov:businessCase-update"
        xmlns:businessCase-type="urn:us:gov:businessCase-type"
        xmlns:businessCase-facet="urn:us:gov:businessCase-facet"
        xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:businessCase-update="urn:us:gov:businessCase-update"
        version="FY_2018">
  <annotation>
    <documentation>
      1. This schema SHOULD NOT BE directly used to Submit Data to ITDashboard.
      2. Any dates that are referred in this schema should be in 'YYYY-MM-DD' format.
    </documentation>
  </annotation>

  <import namespace="urn:us:gov:businessCase-type" schemaLocation="../Subset/businessCaseTypes.xsd"/>
  <import namespace="urn:us:gov:businessCase-facet" schemaLocation="../Subset/businessCaseFacet.xsd"/>

  <element name="capitalAssetPlan">
    <complexType>
      <sequence>
        <element ref="businessCase-update:capitalAssetSummary"/>
        <element ref="businessCase-update:performanceMeasureReport" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="capitalAssetSummary">
    <complexType>
      <sequence>
        <element ref="businessCase-update:agency"/>
        <element ref="businessCase-update:uniqueInvestmentIdentifier"/>
        <element ref="businessCase-update:investmentDetails" minOccurs="0"/>
        <element ref="businessCase-update:lifeCycleCosts" minOccurs="0"/>
        <element ref="businessCase-update:investmentStartEndDates" minOccurs="0"/>
        <element ref="businessCase-update:changesFromPreviousBudget" minOccurs="0"/>
        <element ref="businessCase-update:acquisitionStrategy" minOccurs="0"/>
        <element ref="businessCase-update:cioRating" minOccurs="0"/>
        <element ref="businessCase-update:systemsInventory" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="investmentDetails">
    <complexType>
      <sequence>
        <element ref="businessCase-update:investmentDescription" minOccurs="0"/>
        <element ref="businessCase-update:objectivesOrGoals" minOccurs="0"/>
        <element ref="businessCase-update:returnOnInvestmentDescription" minOccurs="0"/>
        <element ref="businessCase-update:investmentRequirements" minOccurs="0"/>
        <element ref="businessCase-update:programCodeFPI" minOccurs="0"/>
        <element ref="businessCase-update:investmentsEliminatedorReduced" minOccurs="0"/>
        <element ref="businessCase-update:investmentSharedService" minOccurs="0"/>
        <element ref="businessCase-update:investmentPivEnabled" minOccurs="0"/>
        <element ref="businessCase-update:publicURLs" minOccurs="0"/>
        <element ref="businessCase-update:ombControlNumbers" minOccurs="0"/>
        <element ref="businessCase-update:pmDetails" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="performanceMeasureReport">
    <complexType>
      <sequence>
        <element ref="businessCase-update:projectFundingPlan" minOccurs="0"/>
        <element ref="businessCase-update:reviseInvestment" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:operationalAnalysis" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetrics" minOccurs="0"/>
        <element ref="businessCase-update:investmentRisks" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="investmentsEliminatedorReduced">
    <complexType>
      <sequence>
        <element ref="businessCase-update:addInvestmentEliminatedorReduced" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:updateInvestmentEliminatedorReduced" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <complexType name="addContactInformationType">
    <sequence>
      <element ref="businessCase-update:agencyContactId" minOccurs="0"/>
      <element ref="businessCase-update:contactType"/>
      <element ref="businessCase-update:contactTitle" minOccurs="0"/>
      <element ref="businessCase-update:contactDetail"/>
    </sequence>
  </complexType>

  <complexType name="updateContactInformationType">
    <sequence>
      <element ref="businessCase-update:contactId" minOccurs="0"/>
      <element ref="businessCase-update:agencyContactId" minOccurs="0"/>
      <element ref="businessCase-update:contactTitle" minOccurs="0"/>
      <element ref="businessCase-update:contactDetail" minOccurs="0"/>
      <element ref="businessCase-update:isActive" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="contactDetailType">
    <sequence>
      <element ref="businessCase-update:contactName"/>
      <element ref="businessCase-update:contactNumber" minOccurs="0"/>
      <element ref="businessCase-update:contactNumberExt" minOccurs="0"/>
      <element ref="businessCase-update:contactEmail" minOccurs="0"/>
    </sequence>
  </complexType>

  <element name="lifeCycleCosts">
    <complexType>
      <sequence>
        <element ref="businessCase-update:dmeGovernmentFTECostsPY-1" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsPY" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsCY" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsBY" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsBY_1" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsBY_2" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsBY_3" minOccurs="0"/>
        <element ref="businessCase-update:dmeGovernmentFTECostsBY_4_Beyond" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesPY-1" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesPY" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesCY" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesBY" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesBY_1" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesBY_2" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesBY_3" minOccurs="0"/>
        <element ref="businessCase-update:maintenanceBudgetaryResourcesBY_4_Beyond" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsPY-1" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsPY" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsCY" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsBY" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsBY_1" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsBY_2" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsBY_3" minOccurs="0"/>
        <element ref="businessCase-update:omdGovernmentFTECostsBY_4_Beyond" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberPY-1" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberPY" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberCY" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberBY" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberBY_1" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberBY_2" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberBY_3" minOccurs="0"/>
        <element ref="businessCase-update:governmentFTENumberBY_4_Beyond" minOccurs="0"/>
        <element ref="businessCase-update:priorYearCostChangePY" minOccurs="0"/>
        <element ref="businessCase-update:priorYearCostChangeCY" minOccurs="0"/>
        <element ref="businessCase-update:priorYearCostPercentageChangePY" minOccurs="0"/>
        <element ref="businessCase-update:priorYearCostPercentageChangeCY" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="investmentStartEndDates">
    <complexType>
      <sequence>
        <element ref="businessCase-update:investmentStartYear"/>
        <element ref="businessCase-update:investmentEndYear"/>
      </sequence>
    </complexType>
  </element>

  <element name="publicURLs">
    <complexType>
      <sequence>
        <element ref="businessCase-update:publicURL" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="ombControlNumbers">
    <complexType>
      <sequence>
        <element ref="businessCase-update:ombControlNumber" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="pmDetails">
    <complexType>
      <sequence>
        <element ref="businessCase-update:pmName"/>
        <element ref="businessCase-update:pmEmail"/>
        <element ref="businessCase-update:pmQualifications"/>
      </sequence>
    </complexType>
  </element>


  <element name="acquisitionStrategy">
    <complexType>
      <sequence>
        <element ref="businessCase-update:existingContracts" minOccurs="0"/>
        <element ref="businessCase-update:plannedAcquisitions" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <complexType name="attachment">
    <sequence>
      <element ref="businessCase-update:title"/>
      <element ref="businessCase-update:binaryObject"/>
    </sequence>
  </complexType>

  <complexType name="binaryType">
    <sequence>
      <element ref="businessCase-update:binaryObject"/>
    </sequence>
  </complexType>

  <complexType name="base64Binary">
    <simpleContent>
      <extension base="base64Binary">
        <attribute name="contentType" type="string" use="required"/>
      </extension>
    </simpleContent>
  </complexType>

  <element name="existingContracts">
    <complexType>
      <sequence>
        <element minOccurs="0" maxOccurs="unbounded" ref="businessCase-update:updateContract"/>
        <element minOccurs="0" maxOccurs="unbounded" ref="businessCase-update:addContract"/>
      </sequence>
    </complexType>
  </element>

  <element name="plannedAcquisitions">
    <complexType>
      <sequence>
        <element minOccurs="0" maxOccurs="unbounded" ref="businessCase-update:updateAcquisition"/>
        <element minOccurs="0" maxOccurs="unbounded" ref="businessCase-update:addAcquisition"/>
      </sequence>
    </complexType>
  </element>

  <complexType name="replacedContractsType">
    <sequence>
      <element ref="businessCase-update:updateReplacedContract" minOccurs="0" maxOccurs="unbounded"/>
      <element ref="businessCase-update:addReplacedContract" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="addReplacedContractType">
    <sequence>
      <element ref="businessCase-update:agencyReplacedContractId" minOccurs="0"/>
      <element ref="businessCase-update:contractNumber"/>
      <element ref="businessCase-update:referencedPIID" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="updateReplacedContractType">
    <sequence>
      <element ref="businessCase-update:replacedContractId" minOccurs="0"/>
      <element ref="businessCase-update:agencyReplacedContractId" minOccurs="0"/>
      <element ref="businessCase-update:contractNumber" minOccurs="0"/>
      <element ref="businessCase-update:referencedPIID" minOccurs="0"/>
      <element ref="businessCase-update:isActive" minOccurs="0"/>
    </sequence>
  </complexType>

  <element name="addContract">
    <complexType>
      <sequence>
        <element ref="businessCase-update:agencyContractId" minOccurs="0"/>
        <element ref="businessCase-update:contractNumber"/>
        <element ref="businessCase-update:referencedPIID" minOccurs="0"/>
        <element ref="businessCase-update:isModularApproach"/>
        <element ref="businessCase-update:agileDevelopment"/>
        <element ref="businessCase-update:contractPurpose"/>
        <element ref="businessCase-update:ITLeaseCategory" minOccurs="0"/>
        <element ref="businessCase-update:informationSecurityClause"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateContract">
    <complexType>
      <sequence>
        <element ref="businessCase-update:contractId" minOccurs="0"/>
        <element ref="businessCase-update:agencyContractId" minOccurs="0"/>
        <element ref="businessCase-update:contractNumber" minOccurs="0"/>
        <element ref="businessCase-update:referencedPIID" minOccurs="0"/>
        <element ref="businessCase-update:isModularApproach" minOccurs="0"/>
        <element ref="businessCase-update:agileDevelopment" minOccurs="0"/>
        <element ref="businessCase-update:contractPurpose" minOccurs="0"/>
        <element ref="businessCase-update:ITLeaseCategory" minOccurs="0"/>
        <element ref="businessCase-update:informationSecurityClause" minOccurs="0"/>
        <element ref="businessCase-update:isActive" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="addAcquisition">
    <complexType>
      <sequence>
        <element ref="businessCase-update:agencyAcquisitionId" minOccurs="0"/>
        <element ref="businessCase-update:plannedContractDescription"/>
        <element ref="businessCase-update:plannedAwardDate"/>
        <element ref="businessCase-update:solicitationId" minOccurs="0"/>
        <element ref="businessCase-update:lengthOfPlannedSupport"/>
        <element ref="businessCase-update:anticipatedValue"/>
        <element ref="businessCase-update:isModularApproach"/>
        <element ref="businessCase-update:agileDevelopment"/>
        <element ref="businessCase-update:potentialSources"/>
        <element ref="businessCase-update:providerEngagement"/>
        <element ref="businessCase-update:containsITLease"/>
        <element ref="businessCase-update:replacedContracts" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateAcquisition">
    <complexType>
      <sequence>
        <element ref="businessCase-update:acquisitionId" minOccurs="0"/>
        <element ref="businessCase-update:agencyAcquisitionId" minOccurs="0"/>
        <element ref="businessCase-update:plannedContractDescription" minOccurs="0"/>
        <element ref="businessCase-update:plannedAwardDate" minOccurs="0"/>
        <element ref="businessCase-update:solicitationId" minOccurs="0"/>
        <element ref="businessCase-update:lengthOfPlannedSupport" minOccurs="0"/>
        <element ref="businessCase-update:anticipatedValue" minOccurs="0"/>
        <element ref="businessCase-update:isModularApproach" minOccurs="0"/>
        <element ref="businessCase-update:agileDevelopment" minOccurs="0"/>
        <element ref="businessCase-update:potentialSources" minOccurs="0"/>
        <element ref="businessCase-update:providerEngagement" minOccurs="0"/>
        <element ref="businessCase-update:containsITLease" minOccurs="0"/>
        <element ref="businessCase-update:replacedContracts" minOccurs="0"/>
        <element ref="businessCase-update:isActive" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="performanceMetrics">
    <complexType>
      <sequence>
        <element ref="businessCase-update:addPerformanceMetric" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:updatePerformanceMetric" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="updatePerformanceMetric">
    <complexType>
      <sequence>
        <element ref="businessCase-update:performanceMetricId" minOccurs="0"/>
        <element ref="businessCase-update:agencyPerformanceMetricId" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricCategory" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricBaselineCapability" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricCYTarget" minOccurs="0"/>
        <element ref="businessCase-update:performancePeriodicity" minOccurs="0"/>
        <element ref="businessCase-update:addPerformanceMetricActual" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:performanceMetricObjectiveOrGoal" minOccurs="0"/>
        <element ref="businessCase-update:isClosed" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="addPerformanceMetricActual">
    <complexType>
      <sequence>
        <element ref="businessCase-update:agencyPerformanceMetricActualID" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricActualResult"/>
        <element ref="businessCase-update:performanceMetricDateofActual"/>
        <element ref="businessCase-update:performanceMetricComment" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="reviseUpdatePerformanceMetricActual">
    <complexType>
      <sequence>
        <element ref="businessCase-update:performanceMetricActualID" minOccurs="0"/>
        <element ref="businessCase-update:agencyPerformanceMetricActualID" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricActualResult" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricDateofActual" minOccurs="0"/>
        <element ref="businessCase-update:performanceMetricComment" minOccurs="0"/>
        <element ref="businessCase-update:isActive" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="investmentRisks">
    <complexType>
      <sequence>
        <element ref="businessCase-update:updateInvestmentRisk" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:addInvestmentRisk" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateInvestmentRisk">
    <complexType>
      <sequence>
        <element ref="businessCase-update:investmentRiskId" minOccurs="0"/>
        <element ref="businessCase-update:agencyInvestmentRiskId" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskName" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskCategory" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskProbability" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskImpact" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskMitigationPlan" minOccurs="0"/>
        <element ref="businessCase-update:isActive" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="addInvestmentRisk">
    <complexType>
      <sequence>
        <element ref="businessCase-update:agencyInvestmentRiskId" minOccurs="0"/>
        <element ref="businessCase-update:investmentRiskName"/>
        <element ref="businessCase-update:investmentRiskCategory"/>
        <element ref="businessCase-update:investmentRiskProbability"/>
        <element ref="businessCase-update:investmentRiskImpact"/>
        <element ref="businessCase-update:investmentRiskMitigationPlan"/>
      </sequence>
    </complexType>
  </element>

  <element name="projectFundingPlan">
    <complexType>
      <sequence>
        <element ref="businessCase-update:projects"/>
      </sequence>
    </complexType>
  </element>

  <element name="projects">
    <complexType>
      <sequence>
        <element ref="businessCase-update:addProject" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:updateProject" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateProject">
    <complexType>
      <sequence>
        <element ref="businessCase-update:projectId" minOccurs="0"/>
        <element ref="businessCase-update:agencyProjectId" minOccurs="0"/>
        <element ref="businessCase-update:updateProjectDetail" minOccurs="0"/>
        <element ref="businessCase-update:updateProjectActivities" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateProjectDetail">
    <complexType>
      <sequence>
        <element ref="businessCase-update:SDLCmethodology" minOccurs="0"/>
        <element ref="businessCase-update:releaseEverySixMo" minOccurs="0"/>
        <element ref="businessCase-update:releaseEverySixMoComment" minOccurs="0"/>
        <element ref="businessCase-update:productionDeploymentDate" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateProjectActivities">
    <complexType>
      <sequence>
        <element ref="businessCase-update:updateProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:addProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:updateAgileProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:addAgileProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateProjectActivity">
    <complexType>
      <sequence>
        <element ref="businessCase-update:activityId" minOccurs="0"/>
        <element ref="businessCase-update:agencyActivityId" minOccurs="0"/>
        <element ref="businessCase-update:activityType" minOccurs="0"/>
        <element ref="businessCase-update:criticalPath" minOccurs="0"/>
        <element ref="businessCase-update:projectedStartDate" minOccurs="0"/>
        <element ref="businessCase-update:actualStartDate" minOccurs="0"/>
        <element ref="businessCase-update:projectedCompletionDate" minOccurs="0"/>
        <element ref="businessCase-update:actualCompletionDate" minOccurs="0"/>
        <element ref="businessCase-update:projectedTotalCost" minOccurs="0"/>
        <element ref="businessCase-update:actualTotalCost" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>

  <element name="updateAgileProjectActivity">
    <complexType>
      <sequence>
        <element ref="businessCase-update:activityId" minOccurs="0"/>
        <element ref="businessCase-update:agencyActivityId" minOccurs="0"/>
        <element ref="businessCase-update:releaseName" minOccurs="0"/>
        <element ref="businessCase-update:releaseNumber" minOccurs="0"/>
        <element ref="businessCase-update:releaseDescription" minOccurs="0"/>
        <element ref="businessCase-update:NPI" minOccurs="0"/>
        <element ref="businessCase-update:NPE" minOccurs="0"/>
        <element ref="businessCase-update:NCE" minOccurs="0"/>
        <element ref="businessCase-update:NCI" minOccurs="0"/>
        <element ref="businessCase-update:DTC" minOccurs="0"/>
        <element ref="businessCase-update:DPC" minOccurs="0"/>
        <element ref="businessCase-update:plannedStartDate" minOccurs="0"/>
        <element ref="businessCase-update:actualStartDate" minOccurs="0"/>
        <element ref="businessCase-update:plannedCompletionDate" minOccurs="0"/>
        <element ref="businessCase-update:actualCompletionDate" minOccurs="0"/>
        <element ref="businessCase-update:plannedTotalCost" minOccurs="0"/>
       <element ref="businessCase-update:actualTotalCost" minOccurs="0"/>
      </sequence>
    </complexType>
  </element>
  
  <element name="reviseInvestment">
    <complexType>
      <sequence>
        <element ref="businessCase-update:baselineApprovalDate"/>
        <element ref="businessCase-update:reviseOption"/>
        <element ref="businessCase-update:reviseComments"/>
        <element ref="businessCase-update:reviseAddProject" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:reviseUpdateProject" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:reviseAddPerformanceMetric" minOccurs="0" maxOccurs="unbounded"/>
        <element ref="businessCase-update:reviseUpdatePerformanceMetric" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
      <attribute name="id" type="string" use="required"/>
    </complexType>
  </element>

  <complexType name="addProjectType">
    <sequence>
      <element ref="businessCase-update:agencyProjectId" minOccurs="0"/>
      <element ref="businessCase-update:addProjectDetail"/>
      <element ref="businessCase-update:addProjectActivities"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectType">
    <sequence>
      <element ref="businessCase-update:agencyProjectId" minOccurs="0"/>
      <element ref="businessCase-update:reviseAddProjectDetail"/>
      <element ref="businessCase-update:reviseAddProjectActivities"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectDetailType">
    <sequence>
      <element ref="businessCase-update:uniqueProjectId"/>
      <element ref="businessCase-update:projectName"/>
      <element ref="businessCase-update:objectivesExpectedOutcomes"/>
      <element ref="businessCase-update:projectStartDate"/>
      <element ref="businessCase-update:projectCompletionDate"/>
      <element ref="businessCase-update:projectLifeCycleCost"/>
      <element ref="businessCase-update:SDLCmethodology" minOccurs="0"/>
      <element ref="businessCase-update:releaseEverySixMo" minOccurs="0"/>
      <element ref="businessCase-update:releaseEverySixMoComment" minOccurs="0"/>
      <element ref="businessCase-update:productionDeploymentDate" minOccurs="0"/>
    </sequence>
  </complexType>

  <complexType name="addProjectActivitiesType">
    <sequence>
      <element ref="businessCase-update:addProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
      <element ref="businessCase-update:addAgileProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectActivitiesType">
    <sequence>
      <element ref="businessCase-update:reviseAddProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
      <element ref="businessCase-update:reviseAddAgileProjectActivity" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>
  </complexType>

  <complexType name="reviseAddProjectActivityType">
    <sequence>
      <element ref="businessCase-update:agencyActivityId" minOccurs="0"/>
      <element ref="businessCase-update:activityName"/>
      <element ref="businessCase-update:activityDescription" minOccurs="0"/>
      <element ref="businessCase-update:structureId"/>
      <element ref="businessCase-update:activityType" minOccurs="0"/>
