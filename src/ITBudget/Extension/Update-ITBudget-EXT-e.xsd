    <xsd:element name="AgencyCode" type="ITBudgetFacet:AgencyCodeType">
        <xsd:annotation>
            <xsd:documentation>The assigned code of a particuliar agency</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AgencyContributionAmount" type="nc:AmountType">
        <xsd:annotation>
            <xsd:documentation>Both monetary contributions and fees for services provided by partner agencies to managing partners or shared service providers of multi-agency collaboration.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AgencyFundingAmount" type="nc:AmountType">
        <xsd:annotation>
            <xsd:documentation>Agency's authorized funding for a given investment.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AgencyReasonforChange" type="nc:TextType">
        <xsd:annotation>
            <xsd:documentation>Agency's description of the reason for changing the investment.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AlternativeBureauCode" type="ITBudgetFacet:BureauCodeType">
        <xsd:annotation>
            <xsd:documentation>Alternative Bureau Code is only required when specifying a bureau for this funding source that is different than bureau specified in the MAX Funding Account.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BudgetYear" type="update-ITBudget-ext:AnnualBudgetType">
        <xsd:annotation>
            <xsd:documentation>A complex type used to contain information about an agency's spending in a budget year for particuliar investment.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BureauCode" type="ITBudgetFacet:BureauCodeType">
        <xsd:annotation>
            <xsd:documentation>Two digits are your bureau code. If this is a department only reporting or an agency-wide activity, use 00 as your bureau code. </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="BudgetYearAmount" type="nc:AmountType">
        <xsd:annotation>
            <xsd:documentation>TBD</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CloudComputingAlternativesEvaluation"
                 type="ITBudgetFacet:CloudComputingAlternativesEvaluationType">
        <xsd:annotation>
            <xsd:documentation>TBD</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
