        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SummaryOandM" type="update-ITBudget-ext:AnnualLineItemType">
        <xsd:annotation>
            <xsd:documentation>TBD</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="SummaryDME" type="update-ITBudget-ext:AnnualLineItemType">
