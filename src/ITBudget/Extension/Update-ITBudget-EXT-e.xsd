    <xsd:element name="CurrentUII" type="nc:UIIType">
        <xsd:annotation>
            <xsd:documentation>The unique investment identifier used to report the investment. Indicating the UII used for the budget process allows cross-walk and historical analysis crossing fiscal years for tracking purposes. This current UII is mandatory.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="CurrentYear" type="update-ITBudget-ext:AnnualBudgetType">
        <xsd:annotation>
            <xsd:documentation>An element used to describe an agency's spending during the current year for a particuliar investment.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
