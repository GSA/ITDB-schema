                    <xsd:element ref="update-ITBudget-ext:GovernmentFTECostForTotalCY"/>
                    <xsd:element ref="update-ITBudget-ext:GovernmentFTENumberForCY"/>
                    <xsd:element ref="update-ITBudget-ext:GovernmentFTECostForTotalBY"/>
                    <xsd:element ref="update-ITBudget-ext:GovernmentFTENumberForBY"/>
                    <xsd:element ref="update-ITBudget-ext:CloudComputingAlternativesEvaluation" minOccurs="0"/>
                    <xsd:element ref="update-ITBudget-ext:ProvisionedITSpending"/>
                    <xsd:element ref="update-ITBudget-ext:SecuritySpending"/>
                    <xsd:element ref="update-ITBudget-ext:EndofLifeSpendingCY" minOccurs="0"/>
                    <xsd:element ref="update-ITBudget-ext:FundingSourceLineItems"/>
                </xsd:sequence>
